{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && npm run i18n:extract && next build",
    "start": "next start",
    "lint": "next lint",
    "vercel-build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "storybook": "npm run fix-storybook && storybook dev -p 6006",
    "build-storybook": "npm run fix-storybook && storybook build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "upload-documents": "tsx scripts/upload-sample-documents.ts",
    "organize-storybook": "node scripts/organize-storybook.js",
    "fix-storybook": "node scripts/fix-storybook-dependencies.js",
    "test-rls": "NODE_OPTIONS=--experimental-vm-modules jest __tests__/security/",
    "whatsapp:test": "tsx scripts/whatsapp-baileys-test.ts",
    "whatsapp:baileys": "tsx scripts/whatsapp-baileys-test.ts",
    "whatsapp:simple": "tsx scripts/whatsapp-simple-baileys-test.ts",
    "whatsapp:meta": "tsx scripts/whatsapp-meta-test.ts",
    "whatsapp:debug": "tsx scripts/whatsapp-debug-test.ts",
    "whatsapp:send": "tsx scripts/whatsapp-send-test.ts",
    "whatsapp:test-assistant": "tsx scripts/test-whatsapp-assistant.ts",
    "validate-naming": "node scripts/validate-prisma-naming.js",
    "build-safe": "npm run validate-naming && prisma generate && next build",
    "precommit-check": "npm run validate-naming && npm run lint && npm run i18n:check",
    "translation:worker": "node scripts/translation-worker.js",
    "translation:worker:once": "node scripts/translation-worker.js --once",
    "translation:worker:start": "node scripts/translation-worker.js",
    "i18n:extract": "i18next --config i18next-parser.config.js",
    "i18n:validate": "i18next --config i18next-parser.config.js --silent && node scripts/validate-translations.js",
    "i18n:check": "npm run i18n:extract -- --fail-on-update && npm run i18n:validate",
    "i18n:check-strict": "npm run i18n:extract && npm run i18n:validate && npm run i18n:find-hardcoded",
    "i18n:find-hardcoded": "node scripts/find-hardcoded-text.js",
    "i18n:stats": "node scripts/translation-stats.js",
    "i18n:report": "npm run i18n:extract && npm run i18n:stats"
  },
